<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="md.leonis.watcher.view.BookmarksController">
   <center>
      <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="600.0" prefWidth="700.0" BorderPane.alignment="CENTER">
        <items>
            <TableView fx:id="bookmarksTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="100.0" minWidth="-Infinity">
              <columns>
                <TableColumn fx:id="folderColumn" prefWidth="75.0" text="C1" />
                <TableColumn fx:id="totalColumn" prefWidth="75.0" text="C2" />
              </columns>
            </TableView>
            <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Web View">
                     <content>
                        <BorderPane prefHeight="200.0" prefWidth="200.0">
                           <top>
                              <HBox alignment="CENTER_LEFT" prefHeight="37.0" prefWidth="700.0" spacing="5.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Button mnemonicParsing="false" text="Button" />
                                 </children>
                                 <padding>
                                    <Insets left="5.0" />
                                 </padding>
                              </HBox>
                           </top>
                           <bottom>
                              <HBox alignment="CENTER_LEFT" prefHeight="39.0" prefWidth="700.0" spacing="5.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Label text="Label" />
                                 </children>
                                 <padding>
                                    <Insets left="5.0" />
                                 </padding>
                              </HBox>
                           </bottom>
                           <center>
                              <WebView fx:id="webView" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER" />
                           </center>
                        </BorderPane>
                     </content>
                </Tab>
                <Tab text="Table View">
                     <content>
                        <BorderPane prefHeight="200.0" prefWidth="200.0">
                           <top>
                              <HBox alignment="CENTER_LEFT" prefHeight="37.0" prefWidth="700.0" spacing="5.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Button mnemonicParsing="false" text="Button" />
                                 </children>
                                 <padding>
                                    <Insets left="5.0" />
                                 </padding>
                              </HBox>
                           </top>
                           <bottom>
                              <HBox alignment="CENTER_LEFT" prefHeight="39.0" prefWidth="700.0" spacing="5.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Label text="Label" />
                                 </children>
                                 <padding>
                                    <Insets left="5.0" />
                                 </padding>
                              </HBox>
                           </bottom>
                           <center>
                              <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                           </center>
                        </BorderPane>
                     </content>
                </Tab>
                  <Tab disable="true" text="Tree View">
                     <content>
                        <BorderPane prefHeight="200.0" prefWidth="200.0">
                           <top>
                              <HBox alignment="CENTER_LEFT" prefHeight="37.0" prefWidth="700.0" spacing="5.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Button mnemonicParsing="false" text="Button" />
                                 </children>
                                 <padding>
                                    <Insets left="5.0" />
                                 </padding>
                              </HBox>
                           </top>
                           <bottom>
                              <HBox alignment="CENTER_LEFT" prefHeight="39.0" prefWidth="700.0" spacing="5.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Label text="Label" />
                                 </children>
                                 <padding>
                                    <Insets left="5.0" />
                                 </padding>
                              </HBox>
                           </bottom>
                        </BorderPane>
                     </content>
                  </Tab>
              </tabs>
            </TabPane>
        </items>
      </SplitPane>
   </center>
</BorderPane>
